            <img class="activate-button" src="activate.png"></img>
        <!-- </div> -->
    <!-- </div> -->

    <a-scene embedded arjs vr-mode-ui="enabled: false">
      <a-marker preset="hiro">
      </a-marker>
      <a-camera look-controls-enabled="true" look-controls="touchEnabled: false;"></a-camera>
    </a-scene>
  </body>
</html>